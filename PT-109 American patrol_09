# PT-109 Message Decoder

def playfair_decrypt(ciphertext, key):
    # Create the Playfair matrix
    matrix = create_playfair_matrix(key)
    plaintext = ""
    
    # Decrypt the ciphertext
    for i in range(0, len(ciphertext), 2):
        digraph = ciphertext[i:i+2]
        plaintext += decrypt_digraph(digraph, matrix)
    
    return plaintext

def create_playfair_matrix(key):
    # Create a 5x5 matrix for Playfair cipher
    key = ''.join(sorted(set(key), key=key.index)).replace('J', 'I')
    alphabet = 'ABCDEFGHIKLMNOPQRSTUVWXYZ'
    matrix = []
    
    for char in key + alphabet:
        if char not in matrix:
            matrix.append(char)
    
    return [matrix[i:i+5] for i in range(0, 25, 5)]

